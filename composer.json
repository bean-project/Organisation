{
  "name": "bean-project/organisation",
  "type": "library",
  "description": "Bean Organisation Component",
  "keywords": ["organisation manager","company management"],
  "homepage": "http://bean-project.org",
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Pierre",
      "email": "peter@bean-project.org"
    }
  ],
  "require": {
    "php": "^5.6|^7.0",

    "sylius/resource": "^1.0",
    "symfony/intl": "^3.2"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpspec/phpspec": "^3.2"
  },
  "config": {
    "bin-dir": "bin"
  },
  "suggest": {
    "sonata-project/doctrine-orm-admin-bundle": "~2.3",
    "liip/imagine-bundle": "~0.9"
  },
  "conflict": {
    "jms/serializer": "<0.13",
    "sonata-project/seo-bundle": "<1.1.5",
    "sonata-project/block-bundle": "<2.2.8"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": { "Bean\\Component\\Organisation\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
